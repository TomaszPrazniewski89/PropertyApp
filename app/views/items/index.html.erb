
 <div class="container">

<table>

  <tbody>
<% @items.each do |item| %>
    <tr>
      <td><%=link_to image_tag(item.image.url, size: "50x50") , type_category_item_path(id: item.id)%></td>
      <td><div class="divider"> </div></td>
      <td><%= link_to item.title, type_category_item_path(id: item.id) %></td>
    </tr>
<% end %>
</tbody>
</table>
</div>
